    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.3",
    "react-redux": "^9.2.0",
    "react-resizable": "^3.0.5",
    "react-syntax-highlighter": "^15.6.1",
    "react-transition-group": "^4.4.5",
    "rxjs": "^7.8.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "ssh2": "^1.16.0",
    "ssh2-sftp-client": "^11.0.0",
    "tree-sitter": "^0.21.1",
    "tree-sitter-bash": "^0.23.3",
    "web-tree-sitter": "^0.24.7",
