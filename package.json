{
  "name": "ai-ssh-tool",
  "version": "1.0.0",
  "description": "An AI-powered SSH client",
  "author": "Developer",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "concurrently \"npm run start:renderer\" \"cross-env NODE_ENV=development npm run start:main\"",
    "start:fast": "concurrently --kill-others-on-fail \"npm run start:renderer:fast\" \"cross-env NODE_ENV=development npm run start:main\"",
    "start:renderer:fast": "webpack serve --config webpack.fast.js",
    "start:main": "tsc -p tsconfig.main.json --incremental && electron .",
    "start:renderer": "webpack serve --config webpack.dev.js",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "webpack --config webpack.prod.js",
    "pack": "npm run build && cross-env ELECTRON_BUILDER_CACHE=node_modules/.cache/electron electron-builder --dir",
    "dist": "npm run build && cross-env ELECTRON_BUILDER_CACHE=node_modules/.cache/electron electron-builder",
    "dist:win": "npm run build && cross-env ELECTRON_BUILDER_CACHE=node_modules/.cache/electron electron-builder --win",
    "postinstall": "electron-builder install-app-deps",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:startup": "node scripts/test-startup.js"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@ant-design/plots": "^2.3.3",
    "@electron/node-gyp": "^10.2.0-electron.2",
    "@reduxjs/toolkit": "^2.5.0",
    "@types/react-resizable": "^3.0.8",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/react-transition-group": "^4.4.12",
    "@types/ssh2": "^1.15.3",
    "@types/ssh2-sftp-client": "^9.0.4",
    "@types/xterm": "^2.0.3",
    "antd": "^5.24.1",
    "better-sqlite3": "^8.7.0",
    "chokidar": "^3.5.3",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "generic-pool": "^3.9.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "iconv-lite": "^0.6.3",
    "jschardet": "^3.1.4",
    "mobx": "^6.12.0",
    "mobx-react": "^9.1.0",
    "monaco-editor": "^0.45.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.78.1",
